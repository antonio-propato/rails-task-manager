<h1>Edit task</h1>
<div class="links">
<%= form_with model: @task do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title%>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.check_box :completed %>
  <%= f.label :'Completed' %>
  <%= f.submit %>
<% end %>


<%= link_to 'Back', tasks_path %>
</div>
